#CLEAR

NB: 
- untuk mengubah test tertentu pada sebuah materi di batch dan training tertentu


Header
- Content-Type  : application/json
- Authorization : Bearer <token>


url      : http://localhost:{port}/api/admin/test
type     : PUT
params   : {
    "batchId": "JAN-2020",
    "training": "4",
    "materialId": "2",
    "materialName": "Think Win Win",
    "available": "15-02-2020",
    "closed": "18-02-2020",
    "timeLimit": "20",
    "questions": [
        {
            "questionText": "Apa yang dimaksud dengan gerak bumi?",
            "choices": [
                "Gaya yang bergerak di bumi",
                "Gerak yang putus asa",
                "Dunia yang bergerak",
                "Semua salah",
            ],
            "correctAnswer": "3",
        },
        {
            "questionText": "Apa yang dikelola oleh Blibli?",
            "choices": [
                "Makanan",
                "Minuman",
                "Snack",
                "Daging hidup",
            ],
            "correctAnswer": "1",
        },
    ],
}


RESPONSE
--------
{
	"code": 202,
	"status": "ACCEPTED",
	"data": {
        "status": "ACCEPTED",
        "message": "Test has been updated"
    },
    "paging": null,
    "errors": null,
}