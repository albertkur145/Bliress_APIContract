NB: 
- untuk mengambil semua materi test di batch dan training tertentu.


url      : http://localhost:{port}/api/admin/test
type     : GET
params   : {
    "batch": "2",
    "training": "4",
}


RESPONSE
--------
{
	"code": 200,
	"status": "OK",
	"data": {
        "batch" : "2",
        "training": "4",
        "material": [
            {
                "id": 1,
                "name": "Private Victory",
                "available": "12 September 2020",
                "closed": "15 September 2020",
                "timeLimit": "20",
            },
            {
                "id": 2,
                "name": "Emotional Banking",
                "available": "20 September 2020",
                "closed": "22 September 2020",
                "timeLimit": "60",
            },
        ],
    },
	"paging": null,
